-if @rss_feeds.empty?
    $('#load_more_link').replaceWith("No older feed to show.");
-else
    $('#rss_feeds').append("#{escape_javascript(render 'rss_feeds')}");
    $('#load_more_link').replaceWith("#{escape_javascript(link_to 'Load Older feeds', root_path(page: params[:page].to_i.succ), remote: true, id: 'load_more_link')}");